<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/back</item>
        <item name="colorPrimaryDark">@color/back</item>
        <item name="colorAccent">@color/white</item>
    </style>

    <style name="floatlabelededittext">
        <item name="android:textColor">#D81B60</item>
        <item name="android:textSize">20sp</item>
    </style>
    <!--下划线-->
    <style name="MyEditText" parent="Theme.AppCompat.Light">
        <item name="colorControlNormal">@color/white</item>
        <item name="colorControlActivated">@color/white</item>
    </style>
    <style name="labels_style">
        <item name="android:textSize">20dp</item>
        <item name="android:textColor">@color/turquoise</item>
    </style>
</resources>
